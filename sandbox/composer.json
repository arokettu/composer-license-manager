{
    "name": "sandfox.dev/sandbox",
    "repositories": [
        {
            "type": "path",
            "version": "dev-master",
            "url": ".."
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "license": "0BSD",
    "require": {
        "arokettu/composer-license-manager": "dev-master",
        "laminas/laminas-code": "^4.5",
        "symfony/string": "^6.1",
        "npm-asset/jquery-form": "^4.3"
    },
    "config": {
        "allow-plugins": {
            "arokettu/composer-license-manager": true
        }
    },
    "extra": {
        "arokettu/composer-license-manager": {
            "licenses": {
                "allowed": ["MIT", "BSD-*", "ISC", "artistic-2.0", "apache-2.0", "(wtfpl*"],
                "forbidden": ["BSD-3-Clause"],
                "allow-empty": false
            },
            "packages": {
                "allowed": ["laminas/laminas-code", "npm-asset/deep-extend"]
            },
            "enforced": false
        }
    }
}
